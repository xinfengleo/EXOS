                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    >
                <TextView
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:background="@color/title_backColor"
                    android:text="掛機"
                    android:padding="5dp"
                    />
                <!--提示語-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingLeft="15dp">
                    <TextView
                        style="?android:checkboxStyle"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/yellow"
                        android:textSize="10dp"
                        android:text="※只有在日常頁面的功能都執行完畢後才會執行掛機頁面設定的功能"
                        />
                </LinearLayout>
                <!--選擇刷圖-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingLeft="6dp">
                    <com.gamebot.botdemo.view.RadioGroupExd
                        android:id="@+id/select_shuatu"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        app:defaultIndex="0"
                        app:listArray="強化本,進化本,裝備任務,BOSS戰"
                        app:uniqueKey="掛機刷圖"
                        />
                </LinearLayout>
                <!--強化本-->
                <LinearLayout
