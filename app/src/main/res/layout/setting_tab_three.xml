                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    >
                    <com.gamebot.sdk.view.TitleDivider
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="20dp"
                        android:paddingRight="20dp"
                        app:title="強化本設定"
                        />
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingLeft="12dp"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="強化本等級："
                            />
                        <LinearLayout
                            android:layout_height="30dp"
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:paddingLeft="5dp"
                            >
                            <com.gamebot.botdemo.view.SpinnerExd
                                style="?android:spinnerStyle"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="#fff"
                                app:defaultIndex="0"
                                app:listArray="第一階,第二階,第三階,第四階,第五階,第六階"
                                app:uniqueKey="強化本等級"
                                app:valueFromList="true" />
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingLeft="12dp"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="選擇隊伍："
                            />
                        <com.gamebot.botdemo.view.SpinnerExd
                            style="?android:spinnerStyle"
                            android:layout_width="wrap_content"
                            android:layout_height="30dp"
                            android:textColor="#fff"
                            app:defaultIndex="0"
                            app:listArray="第一隊,第二隊,第三隊,第四隊,第五隊"
                            app:uniqueKey="強化本選隊伍"
                            app:valueFromList="true" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingLeft="12dp"
                        >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="體力不足時："
                            />
                        <com.gamebot.botdemo.view.SpinnerExd
                            style="?android:spinnerStyle"
                            android:layout_width="wrap_content"
                            android:layout_height="30dp"
                            android:textColor="#fff"
                            app:defaultIndex="0"
                            app:listArray="用體力藥,等待回復體力"
                            app:uniqueKey="強化本體力使用"
                            app:valueFromList="true" />
